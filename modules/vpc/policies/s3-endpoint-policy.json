{
    "Version": "2012-10-17",
    "Statement": [
      {
        "Effect": "Allow",
        "Principal": "*",
        "Action": [
          "s3:ListAllMyBuckets",
          "s3:GetObject",
          "s3:ListBucket",
          "s3:PutObject",
          "s3:DeleteObject"
        ],
        "Resource": [
          "arn:aws:s3:::vault-certificate-2026",
          "arn:aws:s3:::vault-certificate-2026/*",
          "arn:aws:s3:::vault-backend-iamphanan",
          "arn:aws:s3:::vault-backend-iamphanan/*"
        ]
      }
    ]
  }
  